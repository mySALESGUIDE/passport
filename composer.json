{
    "name": "laravel/passport",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "firebase/php-jwt": "~3.0|~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.4|~6.0|~7.0",
        "illuminate/console": "~5.4|~6.0|~7.0",
        "illuminate/container": "~5.4|~6.0|~7.0",
        "illuminate/contracts": "~5.4|~6.0|~7.0",
        "illuminate/database": "~5.4|~6.0|~7.0",
        "illuminate/encryption": "~5.4|~6.0|~7.0",
        "illuminate/http": "~5.4|~6.0|~7.0",
        "illuminate/support": "~5.4|~6.0|~7.0",
        "ort-interactive-gmbh/laravel-couchbase": "~5.4|~6.0|~7.0",
        "league/oauth2-server": "^6.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.1",
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
